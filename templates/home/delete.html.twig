<div class="pop-up-delete">

    <p>Etes vous sur de vouloir tout supprimer</p>
    <form action="{{ path('delete',{id:id})}} " method="post" style="display:inline-block;">
        <input type="hidden" name="_method" value="DELETE">
        <input type="hidden" name="_token" value="{{csrf_token('delete' ~ id) }}">
        <button class="btn btn-danger">Supprimer<i class="material-icons">
                <!--delete -->
            </i></button>
        <button class="btn btn-primary">Non</button>
    </form>

</div>